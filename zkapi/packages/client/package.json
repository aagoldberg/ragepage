{
  "name": "@zkapi/client",
  "version": "0.1.0",
  "description": "zkAPI client SDK for generating zkTLS proofs of cashflow data",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "zkapi",
    "zktls",
    "reclaim",
    "cashflow",
    "oracle",
    "web3"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@reclaimprotocol/js-sdk": "^2.0.0",
    "@noble/curves": "^1.2.0",
    "ethers": "^6.9.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aagoldberg/zkapi.git",
    "directory": "packages/client"
  }
}
